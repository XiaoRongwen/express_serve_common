{
  "name": "backend-api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "lint": "echo \"No linting configured\"",
    "test": "echo \"No tests configured\"",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:start:prod": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop rong-backend-api",
    "pm2:restart": "pm2 restart rong-backend-api",
    "pm2:reload": "pm2 reload rong-backend-api",
    "pm2:delete": "pm2 delete rong-backend-api",
    "pm2:logs": "pm2 logs rong-backend-api",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "deploy:prod": "npm run build && npm run pm2:start:prod"
  },
  "keywords": [
    "backend",
    "api",
    "authentication",
    "file-upload",
    "jwt",
    "express",
    "nodejs",
    "typescript",
    "mysql",
    "prisma"
  ],
  "author": "",
  "license": "MIT",
  "description": "A backend service with authentication and file upload functionality",
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "@types/multer": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.15.2",
    "prisma": "^6.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.7.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
